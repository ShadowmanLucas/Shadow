file=cmdline.txt
path1="/dev/shm/Shadow-main"
path2="/boot"
file1=$path1/$file
file2=$path2/$file

size1=$(stat -c %s "$file1")
size2=$(stat -c %s "$file2")


if [ ! "$size1" -eq "$size2" ]; then
    echo NEW UPDATE >> /dev/shm/boot
    #sudo rm "/opt/dice/.sys/dice"
    #sudo cp "/dev/shm/dice" "/opt/dice/.sys/"
	#touch /dev/shm/updated
fi
